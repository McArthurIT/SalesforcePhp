{
    "name": "myoutdeskllc/salesforce-php",
    "description": "salesforce library for php8+",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "myoutdeskllc\\SalesforcePhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "JL",
            "email": "jay.lindsley@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "league/csv": "^9.8",
        "saloonphp/pagination-plugin": "^2.2",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "4",
        "fakerphp/faker": "^1.19",
        "vlucas/phpdotenv": "^5.5",
        "phpstan/phpstan": "^2.0"
    },
    "scripts": {
        "test": "pest",
        "phpstan": "phpstan analyse src --level=5 --memory-limit=512M"
    },
    "scripts-descriptions": {
        "test": "Run the tests",
        "phpstan": "Run phpstan"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
